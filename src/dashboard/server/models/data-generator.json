{
  "name": "ITS-Data-Generator",
  "things": [
    {
      "id": "data-generator",
      "name": "Super Sensor Data Generator",
      "protocol": "DATABASE",
      "scale": 1,
      "connConfig": {
        "host": "192.168.1.12",
        "port": "27017",
        "user": null,
        "password": null,
        "dbname": "tasdb",
        "options": null
      },
      "sensors": [
        {
          "instanceId": "super-sensor",
          "disable": false,
          "scale": 1,
          "name": "Super Sensor",
          "isFromDatabase": false,
          "userData": {
            "project": "ENACT"
          },
          "options": {},
          "dataSource": {
            "timePeriod": 5,
            "dosAttackSpeedUpRate": 5,
            "timeBeforeFailed": 20,
            "isIPSOFormat": true,
            "sensorBehaviours": [
              "AB_LOW_ENERGY",
              "AB_OUT_OF_ENERGY"
            ],
            "withEnergy": true,
            "energy": {
              "type": "DATA_SOURCE_ENERGY",
              "key": "battery",
              "resourceId": "batter-01",
              "initValue": 1500,
              "unit": "mA",
              "behaviours": [],
              "low": 1000,
              "slowDownRate": 2,
              "consumInOnePeriod": 200
            },
            "sources": [
              {
                "type": "DATA_SOURCE_BOOLEAN",
                "key": "motion_detector",
                "resourceId": "motion-detector-01",
                "initValue": false,
                "unit": null,
                "behaviours": []
              },
              {
                "type": "DATA_SOURCE_ENUM",
                "key": "heating_level",
                "resourceId": "heating_level-01",
                "initValue": "Normal",
                "unit": null,
                "behaviours": [],
                "values": [
                  "Low",
                  "Normal",
                  "High"
                ]
              },
              {
                "type": "DATA_SOURCE_FLOAT",
                "key": "temperature",
                "resourceId": "temp-01",
                "initValue": 20,
                "unit": "C",
                "behaviours": [],
                "valueConstraints": {
                  "min": -273,
                  "max": 100,
                  "regularMin": 0,
                  "regularMax": 35,
                  "step": 0.5
                }
              },
              {
                "type": "DATA_SOURCE_INTEGER",
                "key": "blood_glucose_level",
                "resourceId": "blood-glucose-01",
                "initValue": 5,
                "unit": null,
                "behaviours": [],
                "valueConstraints": {
                  "min": 0,
                  "max": 10,
                  "regularMin": 2,
                  "regularMax": 8,
                  "step": 2
                }
              }
            ]
          }
        }
      ]
    }
  ]
}