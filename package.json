{
  "name": "test_and_simulation",
  "version": "0.1.0",
  "description": "ENACT Test and Simulation Enabler",
  "scripts": {
    "start": "NODE_ENV=production node src/server/app.js",
    "server": "nodemon src/server/app.js"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "*.test.js",
      "Articles/*",
      "models/*",
      "node-red-flows/*",
      "src/client/*",
      "src/public/*",
      "src/server/data/*",
      "src/server/logs/*",
      "src/server/reports/*"
    ],
    "execMap": {
      "rb": "ruby",
      "pde": "processing --sketch={{pwd}} --run"
    }
  },
  "keywords": [
    "Test",
    "Simulation",
    "ENACT",
    "enabler"
  ],
  "author": "Montimage",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "child_process": "~1.0.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.3",
    "crypto": "^1.0.1",
    "debug": "~2.6.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-loader": "^5.1.0",
    "fs": "0.0.1-security",
    "fstream": "^1.0.12",
    "geolib": "^3.2.1",
    "helmet": "^3.21.3",
    "ip": "^1.1.5",
    "jshint": "^2.12.0",
    "lodash": "^4.17.20",
    "mongoose": "^5.9.4",
    "morgan": "^1.9.1",
    "mqtt": "^3.0.0",
    "mqtt-regex": "^1.0.5",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.3",
    "path": "~0.12.7",
    "stompit": "^0.26.0",
    "winston": "^3.2.1",
    "ws": "^7.2.1",
    "zlib": "1.0.5"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "prettier": "^2.0.5"
  }
}
